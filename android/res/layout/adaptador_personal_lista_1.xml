<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="#ffffffff"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ImageView
        android:id="@id/imageView"
        android:layout_width="60.0dip"
        android:layout_height="60.0dip"
        android:layout_marginLeft="0.0dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="false" />
    <TextView
        android:textSize="13.0dip"
        android:textStyle="bold"
        android:id="@+id/titulo"
        android:layout_width="fill_parent"
        android:layout_height="35.0dip"
        android:layout_marginLeft="60.0dip"
        android:text="" />
    <TextView
        android:textSize="13.0dip"
        android:gravity="center"
        android:layout_width="50.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="60.0dip"
        android:layout_marginTop="35.0dip"
        android:text="STOCK: " />
    <TextView
        android:textSize="15.0dip"
        android:textStyle="bold"
        android:textColor="#ffff0000"
        android:gravity="center"
        android:id="@+id/s"
        android:layout_width="40.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="110.0dip"
        android:layout_marginTop="35.0dip"
        android:text="1234" />
    <TextView
        android:textSize="13.0dip"
        android:gravity="center"
        android:layout_width="40.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="150.0dip"
        android:layout_marginTop="35.0dip"
        android:text="P.M. " />
    <TextView
        android:textSize="15.0dip"
        android:textColor="#ff0000ff"
        android:gravity="center|right"
        android:id="@+id/p"
        android:layout_width="50.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="190.0dip"
        android:layout_marginTop="35.0dip"
        android:text="25.00" />
    <TextView
        android:textSize="13.0dip"
        android:gravity="center"
        android:layout_width="40.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="240.0dip"
        android:layout_marginTop="35.0dip"
        android:text="P.C. " />
    <TextView
        android:textSize="15.0dip"
        android:textColor="#ff0000ff"
        android:gravity="center|right"
        android:id="@+id/e"
        android:layout_width="50.0dip"
        android:layout_height="25.0dip"
        android:layout_marginLeft="280.0dip"
        android:layout_marginTop="35.0dip"
        android:text="32.00" />
    <TextView android:gravity="center"
        android:id="@+id/id"
        android:visibility="gone"
        android:layout_width="50.0dip"
        android:layout_height="25.0dip" />
</RelativeLayout>